{
  "name": "webportal",
  "version": "1.0.0",
  "description": "This project will be used to show my portal site.",
  "main": "index.jsx",
  "scripts": {
    "dev:server-org": "webpack-dev-server --content-base src/main/webapp/ --open --host 192.168.1.85 --port 8083",
    "dev:server": "webpack serve  --mode development --content-base src/main/webapp/ --open --host 192.168.1.85 --port 8083",
    "build": "webpack --mode production",
    "develop": "webpack --mode development",
    "prod": "webpack -p",
    "dev": "webpack -d",
    "express": "webpack -d && cp src/main/webapp/index.html src/main/webapp/js/build/index.html && webpack-dev-server --inline --port 9999",
    "test": "webpack-dev-server --content-base src/main/webapp/ --open --inline --hot --port 9999",
    "help": "webpack --help",
    "start": "webpack-dev-server --mode development --open"
  },
